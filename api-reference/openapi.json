    "/intents/address/{address}": {
      "get": {
        "description": "Retrieves an intent using its contract address",
        "operationId": "IntentController_getIntentByAddress",
        "parameters": [
          {
            "name": "address",
            "required": true,
            "in": "path",
            "description": "Intent contract address",
